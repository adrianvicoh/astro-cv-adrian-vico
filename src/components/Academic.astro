---
const { dades } = Astro.props;

const { nom, data, lloc, slug } = dades;
---

<div class="col col-12 col-md-6 mt-4">
  <div class="card bg-dark text-light">
    <div class="card-body">
      <h5 class="card-title">{nom} - {lloc}</h5>
      <p class="card-text">{data}</p>
    </div>
  </div>
</div>
